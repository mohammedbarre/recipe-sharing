<h1>Saved Recipes</h1>
<ul>
    <% savedRecipes.forEach(recipe => { %>
        <li>
            <h3><%= recipe.name %></h3>
            <img src="<%= recipe.thumbnail_url %>" alt="<%= recipe.name %>">
            <p><%= recipe.description || 'No description available.' %></p>
        </li>
    <% }) %>
</ul>
